jQuery(function(e){const d=(t,e,d)=>{e={enterEffect:e,effect:e?t.attr("data-effect-in"):t.attr("data-effect-out"),intensity:e?parseInt(t.attr("data-intensity-in")):parseInt(t.attr("data-intensity-out")),opacity:t.attr("data-effect-opacity"),duration:parseInt(t.attr("data-effect-ms")),triggerItem:d};t.ddb_animate(e,function(t,e){e.enterEffect||(t.parent().hide().closest(".dondivi_hotspot").removeClass("hover"),e.triggerItem.removeClass("active").children(".ddb-multi-value").removeClass("hover active").ddb_set_multi_value())})};e(document).on("mouseleave",".dondivi_hotspot.hover",function(){var t=e(this).find(".ddb-hotspot.ddb-show-hover");t.siblings(".ddb-tooltip").is(":visible")&&d(t.siblings(".ddb-tooltip").children(".ddb-custom-hotspot"),!1,t)}),e(document).on("mouseenter",".ddb-hotspot:not(.active)",function(t){e(this).hasClass("ddb-show-hover")&&(e(this).closest(".dondivi_hotspot").addClass("hover"),d(e(this).siblings(".ddb-tooltip").show().children(".ddb-custom-hotspot"),!0,e(this))),e(this).children(".ddb-multi-value").addClass("hover").ddb_set_multi_value("hover")}),e(document).on("mouseleave",".ddb-hotspot:not(.active):not(.ddb-show-hover)",function(t){e(this).children(".ddb-multi-value").removeClass("hover").ddb_set_multi_value()}),e(document).on("click keydown",".ddb-tooltip-hotspot:not(.ddb-show-hover)",function(t){e(this).hasClass("active")?d(e(this).next().children(".ddb-custom-hotspot"),!1,e(this)):(d(e(this).addClass("active").next().children(".ddb-custom-hotspot"),!0,e(this)),e(this).addClass("active").next().show().children().ddb_animate(),e(this).children(".ddb-multi-value").addClass("active").ddb_set_multi_value("hover"))})});